{
  "name": "check24/feature-flag-bundle",
  "version": "1.1.1",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Toggleable feature flags for your symfony application with various sources like .env, cookie, etc.",
  "authors": [
    {
      "name": "Michael Wolf",
      "email": "wulf1337@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shopping\\FeatureFlagBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "require": {
    "php": "^7.2|^8.0",
    "symfony/config": ">=4.3 <6.0",
    "symfony/dependency-injection": ">=4.3 <6.0",
    "doctrine/annotations": "^1.6",
    "symfony/http-kernel": ">= 4.3 <6.0",
    "twig/twig": ">=2.5 <4.0",
    "symfony/framework-bundle": ">=4.3 <6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "phpstan/phpstan": "^0.12.9"
  }
}
